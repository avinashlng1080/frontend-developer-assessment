{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "semi": 0,
        "comma-dangle": 0,
        "react/jsx-max-props-per-line": [
            true,
            {
                "maximum": 2
            }
        ]
    }
}